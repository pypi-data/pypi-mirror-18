#!/usr/sbin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'PyNews==0.2','console_scripts','pynews'
__requires__ = 'PyNews==0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('PyNews==0.2', 'console_scripts', 'pynews')()
    )
