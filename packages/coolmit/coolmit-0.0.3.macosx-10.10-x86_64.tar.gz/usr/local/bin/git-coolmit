#!/usr/local/opt/python/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'coolmit==0.0.3','console_scripts','git-coolmit'
__requires__ = 'coolmit==0.0.3'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('coolmit==0.0.3', 'console_scripts', 'git-coolmit')()
    )
