#!/home/hk3380/projects/ding/.env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'ding-ding==2.1.0','console_scripts','ding'
__requires__ = 'ding-ding==2.1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ding-ding==2.1.0', 'console_scripts', 'ding')()
    )
