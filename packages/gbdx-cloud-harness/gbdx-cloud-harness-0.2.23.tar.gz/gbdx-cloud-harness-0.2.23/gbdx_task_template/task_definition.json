{
  "containerDescriptors": [
    {
      "type": "DOCKER",
      "properties": {
        "image": "tdgp/cloud-harness:latest"
      }
    }
  ],
  "description": "An Anonymous task for running custom cloud-harness tasks.",
  "inputPortDescriptors": [
    {
      "name": "source_bundle",
      "required": true,
      "type": "directory",
      "description": "Port for the cloud-harness source bundle"
    },
    {
      "name": "dir",
      "required": false,
      "type": "directory",
      "description": "Base directory input port",
      "multiplex": true
    },
    {
      "name": "str",
      "required": false,
      "type": "string",
      "description": "Base string input port",
      "multiplex": true
    }
  ],
  "outputPortDescriptors": [
    {
      "name": "dir",
      "required": false,
      "type": "directory",
      "description": "Base directory input port",
      "multiplex": true
    },
    {
      "name": "str",
      "required": false,
      "type": "string",
      "description": "Base string input port",
      "multiplex": true
    }
  ],
  "properties": {
    "timeout": 7200
  },
  "name": "CloudHarness_Anonymous_Task"
}