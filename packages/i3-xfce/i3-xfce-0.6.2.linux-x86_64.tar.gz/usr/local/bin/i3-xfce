#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'i3-xfce==0.6.2','console_scripts','i3-xfce'
__requires__ = 'i3-xfce==0.6.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('i3-xfce==0.6.2', 'console_scripts', 'i3-xfce')()
    )
