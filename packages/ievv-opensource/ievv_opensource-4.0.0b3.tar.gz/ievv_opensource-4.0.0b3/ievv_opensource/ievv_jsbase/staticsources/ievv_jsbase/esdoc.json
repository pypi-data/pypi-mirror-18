{
  "destination": "built_docs",
  "excludes": [
    "__tests__.+$",
    "__mocks__.+$"
  ],
  "index": "esdoc/index.md",
  "manual": {
    "tutorial": [
      "esdoc/manual/tutorial.md"
    ]
  },
  "plugins": [
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          {
            "from": "^javascript/",
            "to": ""
          },
          {
            "from": "^ievv_jsbase/",
            "to": ""
          }
        ]
      }
    }
  ],
  "source": "scripts/javascript"
}