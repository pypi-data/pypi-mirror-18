prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: LibVNCServer
Description: A library for easy implementation of a VNC server.
Version: 0.9.10
Requires:
Requires.private: zlib
Libs: -L${libdir} -lvncserver
Libs.private: -lpthread -lz -lpng -ljpeg -L/usr/local/Cellar/libgcrypt/1.7.0_1/lib -lgcrypt -L/usr/local/Cellar/libgpg-error/1.22/lib -lgpg-error -L/usr/local/Cellar/gnutls/3.4.12/lib -lgnutls 
Cflags: -I${includedir}

