#!/usr/local/opt/python/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'orlo==0.3.1','console_scripts','orlo'
__requires__ = 'orlo==0.3.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('orlo==0.3.1', 'console_scripts', 'orlo')()
    )
