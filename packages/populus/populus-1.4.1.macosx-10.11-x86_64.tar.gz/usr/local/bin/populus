#!/usr/local/opt/python/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'populus==1.4.1','console_scripts','populus'
__requires__ = 'populus==1.4.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('populus==1.4.1', 'console_scripts', 'populus')()
    )
