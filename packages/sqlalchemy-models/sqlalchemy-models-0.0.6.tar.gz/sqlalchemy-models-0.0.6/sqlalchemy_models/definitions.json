{
  "definitions": {
    "HWBalance": {
      "title": "HWBalance", 
      "required": [
        "available", 
        "total", 
        "currency", 
        "network"
      ], 
      "type": "object", 
      "description": "A Hot Wallet Balance, for internal use only", 
      "properties": {
        "available": {
          "type": "number"
        }, 
        "network": {
          "type": "string", 
          "maxLength": 64
        }, 
        "currency": {
          "type": "string", 
          "maxLength": 4
        }, 
        "time": {
          "type": "string", 
          "format": "date-time"
        }, 
        "total": {
          "type": "number"
        }, 
        "id": {
          "type": "integer"
        }
      }
    }, 
    "Quote": {
      "title": "Quote", 
      "required": [
        "in_amount", 
        "out_amount", 
        "in_address", 
        "out_address", 
        "return_address", 
        "in_currency", 
        "out_currency", 
        "rate"
      ], 
      "type": "object", 
      "description": "A Quote to convert one currency to another.", 
      "properties": {
        "out_amount": {
          "type": "number"
        }, 
        "rate": {
          "type": "number"
        }, 
        "return_address": {
          "type": "string", 
          "maxLength": 80
        }, 
        "in_amount": {
          "type": "number"
        }, 
        "in_currency": {
          "type": "string", 
          "maxLength": 4
        }, 
        "in_address": {
          "type": "string", 
          "maxLength": 80
        }, 
        "id": {
          "type": "integer"
        }, 
        "out_address": {
          "type": "string", 
          "maxLength": 80
        }, 
        "out_currency": {
          "type": "string", 
          "maxLength": 4
        }
      }
    }, 
    "Address": {
      "properties": {
        "network": {
          "type": "string", 
          "maxLength": 64
        }, 
        "address": {
          "type": "string", 
          "maxLength": 64
        }, 
        "currency": {
          "type": "string", 
          "maxLength": 4
        }, 
        "user": {
          "$ref": "#/definitions/User"
        }, 
        "address_state": {
          "enum": [
            "pending", 
            "active", 
            "blocked"
          ], 
          "type": "string", 
          "maxLength": 7
        }, 
        "id": {
          "type": "integer"
        }
      }, 
      "required": [
        "address", 
        "currency", 
        "network", 
        "address_state"
      ], 
      "type": "object", 
      "description": "A payment network Address or account number.", 
      "title": "Address"
    }, 
    "LimitOrder": {
      "required": [
        "price", 
        "amount", 
        "exec_amount", 
        "market", 
        "side", 
        "exchange", 
        "order_id"
      ], 
      "type": "object", 
      "properties": {
        "change_time": {
          "type": "string", 
          "format": "date-time"
        }, 
        "order_id": {
          "type": "string", 
          "maxLength": 80
        }, 
        "price": {
          "type": "number"
        }, 
        "exchange": {
          "type": "string", 
          "maxLength": 12
        }, 
        "state": {
          "enum": [
            "pending", 
            "open", 
            "closed"
          ], 
          "type": "string", 
          "maxLength": 7
        }, 
        "side": {
          "enum": [
            "bid", 
            "ask"
          ], 
          "type": "string", 
          "maxLength": 3
        }, 
        "exec_amount": {
          "type": "number"
        }, 
        "amount": {
          "type": "number"
        }, 
        "create_time": {
          "type": "string", 
          "format": "date-time"
        }, 
        "id": {
          "type": "integer"
        }, 
        "market": {
          "type": "string", 
          "maxLength": 9
        }
      }, 
      "title": "LimitOrder"
    }, 
    "Trade": {
      "required": [
        "trade_id", 
        "exchange", 
        "market", 
        "trade_side", 
        "amount", 
        "price", 
        "fee", 
        "fee_side", 
        "time"
      ], 
      "type": "object", 
      "properties": {
        "fee_side": {
          "type": "string", 
          "maxLength": 5
        }, 
        "trade_side": {
          "enum": [
            "buy", 
            "sell"
          ], 
          "type": "string", 
          "maxLength": 4
        }, 
        "fee": {
          "type": "number"
        }, 
        "exchange": {
          "type": "string", 
          "maxLength": 12
        }, 
        "price": {
          "type": "number"
        }, 
        "trade_id": {
          "type": "string", 
          "maxLength": 80
        }, 
        "amount": {
          "type": "number"
        }, 
        "time": {
          "type": "string", 
          "format": "date-time"
        }, 
        "id": {
          "type": "integer"
        }, 
        "market": {
          "type": "string", 
          "maxLength": 9
        }
      }, 
      "title": "Trade"
    }, 
    "Credit": {
      "properties": {
        "network": {
          "type": "string", 
          "maxLength": 64
        }, 
        "reference": {
          "type": "string", 
          "maxLength": 256
        }, 
        "time": {
          "type": "string", 
          "format": "date-time"
        }, 
        "ref_id": {
          "type": "string", 
          "maxLength": 256
        }, 
        "currency": {
          "type": "string", 
          "maxLength": 4
        }, 
        "amount": {
          "type": "number"
        }, 
        "transaction_state": {
          "enum": [
            "unconfirmed", 
            "complete", 
            "error", 
            "canceled"
          ], 
          "type": "string", 
          "maxLength": 11
        }, 
        "user": {
          "$ref": "#/definitions/User"
        }, 
        "address": {
          "type": "string", 
          "maxLength": 64
        }, 
        "id": {
          "type": "integer"
        }
      }, 
      "required": [
        "amount", 
        "address", 
        "currency", 
        "network", 
        "transaction_state", 
        "ref_id", 
        "time"
      ], 
      "type": "object", 
      "description": "A Credit, which adds tokens to a User's Balance.", 
      "title": "Credit"
    }, 
    "QuoteRequest": {
      "title": "QuoteRequest", 
      "required": [
        "in_amount", 
        "out_amount", 
        "fixed_rate", 
        "out_address", 
        "return_address", 
        "in_currency", 
        "out_currency"
      ], 
      "type": "object", 
      "description": "A Request for a quote to convert one currency to another.", 
      "properties": {
        "out_amount": {
          "type": "number"
        }, 
        "asset_specified": {
          "enum": [
            "in", 
            "out"
          ], 
          "type": "string", 
          "maxLength": 3
        }, 
        "fixed_rate": {
          "type": "boolean"
        }, 
        "in_amount": {
          "type": "number"
        }, 
        "in_currency": {
          "type": "string", 
          "maxLength": 4
        }, 
        "return_address": {
          "type": "string", 
          "maxLength": 80
        }, 
        "id": {
          "type": "integer"
        }, 
        "out_address": {
          "type": "string", 
          "maxLength": 80
        }, 
        "out_currency": {
          "type": "string", 
          "maxLength": 4
        }
      }
    }, 
    "User": {
      "title": "User", 
      "required": [
        "username"
      ], 
      "type": "object", 
      "description": "A User", 
      "properties": {
        "username": {
          "type": "string", 
          "maxLength": 37
        }, 
        "salt": {
          "type": "string", 
          "maxLength": 12
        }, 
        "id": {
          "type": "integer", 
          "description": "primary key"
        }, 
        "createtime": {
          "type": "string", 
          "format": "date-time"
        }
      }
    }, 
    "Debit": {
      "properties": {
        "fee": {
          "type": "number"
        }, 
        "network": {
          "type": "string", 
          "maxLength": 64
        }, 
        "reference": {
          "type": "string", 
          "maxLength": 256
        }, 
        "time": {
          "type": "string", 
          "format": "date-time"
        }, 
        "ref_id": {
          "type": "string", 
          "maxLength": 256
        }, 
        "currency": {
          "type": "string", 
          "maxLength": 4
        }, 
        "amount": {
          "type": "number"
        }, 
        "transaction_state": {
          "enum": [
            "unconfirmed", 
            "complete", 
            "error", 
            "canceled"
          ], 
          "type": "string", 
          "maxLength": 11
        }, 
        "user": {
          "$ref": "#/definitions/User"
        }, 
        "address": {
          "type": "string", 
          "maxLength": 64
        }, 
        "id": {
          "type": "integer"
        }
      }, 
      "required": [
        "amount", 
        "fee", 
        "address", 
        "currency", 
        "network", 
        "transaction_state", 
        "ref_id", 
        "time"
      ], 
      "type": "object", 
      "description": "A Debit, which subtracts tokens from a User's Balance.", 
      "title": "Debit"
    }, 
    "UserKey": {
      "properties": {
        "deactivated_at": {
          "type": "string", 
          "format": "date-time"
        }, 
        "keytype": {
          "enum": [
            "public", 
            "tfa"
          ], 
          "type": "string", 
          "maxLength": 6
        }, 
        "createtime": {
          "type": "string", 
          "format": "date-time"
        }, 
        "permissionbits": {
          "type": "string"
        }, 
        "user": {
          "$ref": "#/definitions/User"
        }, 
        "key": {
          "type": "string", 
          "maxLength": 36
        }, 
        "last_nonce": {
          "type": "string"
        }, 
        "id": {
          "type": "integer", 
          "description": "primary key"
        }
      }, 
      "required": [
        "key", 
        "keytype"
      ], 
      "type": "object", 
      "description": "A User's API key", 
      "title": "UserKey"
    }, 
    "Balance": {
      "properties": {
        "available": {
          "type": "number"
        }, 
        "reference": {
          "type": "string", 
          "maxLength": 256
        }, 
        "currency": {
          "type": "string", 
          "maxLength": 4
        }, 
        "user": {
          "$ref": "#/definitions/User"
        }, 
        "time": {
          "type": "string", 
          "format": "date-time"
        }, 
        "total": {
          "type": "number"
        }, 
        "id": {
          "type": "integer"
        }
      }, 
      "required": [
        "total", 
        "available", 
        "currency"
      ], 
      "type": "object", 
      "description": "A user's balance in a single currency. Only the latest record is valid.", 
      "title": "Balance"
    }, 
    "Ticker": {
      "required": [
        "bid", 
        "ask", 
        "high", 
        "low", 
        "volume", 
        "last", 
        "market", 
        "exchange"
      ], 
      "type": "object", 
      "properties": {
        "volume": {
          "type": "number"
        }, 
        "last": {
          "type": "number"
        }, 
        "exchange": {
          "type": "string", 
          "maxLength": 12
        }, 
        "bid": {
          "type": "number"
        }, 
        "high": {
          "type": "number"
        }, 
        "low": {
          "type": "number"
        }, 
        "time": {
          "type": "string", 
          "format": "date-time"
        }, 
        "ask": {
          "type": "number"
        }, 
        "id": {
          "type": "integer"
        }, 
        "market": {
          "type": "string", 
          "maxLength": 9
        }
      }, 
      "title": "Ticker"
    }, 
    "Payment": {
      "title": "Payment", 
      "required": [
        "out_amount", 
        "out_address", 
        "out_currency"
      ], 
      "type": "object", 
      "description": "A Payment, reflecting that money was sent out.", 
      "properties": {
        "out_currency": {
          "type": "string", 
          "maxLength": 4
        }, 
        "debit_id": {
          "type": "integer"
        }, 
        "id": {
          "type": "integer"
        }, 
        "out_address": {
          "type": "string", 
          "maxLength": 80
        }, 
        "out_amount": {
          "type": "number"
        }
      }
    }
  }
}