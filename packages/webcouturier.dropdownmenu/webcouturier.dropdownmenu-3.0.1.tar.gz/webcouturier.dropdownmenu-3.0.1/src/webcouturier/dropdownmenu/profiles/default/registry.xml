<?xml version="1.0"?>
<registry>
  <records interface="webcouturier.dropdownmenu.browser.interfaces.IDropdownConfiguration" />
  <records
      interface="Products.CMFPlone.interfaces.IResourceRegistry"
      prefix="plone.resources/webcouturier-dropdownmenu">
    <value key="css">
      <element>++plone++dropdown/dropdown.less</element>
    </value>
  </records>
  <records
      interface="Products.CMFPlone.interfaces.IBundleRegistry"
      prefix="plone.bundles/webcouturier-dropdownmenu">
    <value key="resources">
      <element>webcouturier-dropdownmenu</element>
    </value>
    <value key="depends">plone</value>
    <value key="enabled">True</value>
    <value key="compile">True</value>
    <value key="csscompilation">++plone++dropdown/webcouturier.dropdownmenu-compiled.css</value>
    <value key="merge_with">default</value>
  </records>
</registry>
